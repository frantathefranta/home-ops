{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["ceph"],
      "allowedVersions": "<20"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["transmission"],
      "allowedVersions": "<4.0.6 || >4.0.6"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["postgresql"],
      "allowedVersions": "<18"
    }
  ]
}
